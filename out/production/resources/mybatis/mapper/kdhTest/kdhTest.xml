<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prj.web.awesome.KdhTest.mapper.KdhTestMapper">

	<select id="chkUserId" resultType="com.prj.web.awesome.KdhTest.dto.KdhTestDto">
		select
			  user_id			as userId
			 ,user_name			as userName
		from user
		where user_id = #{userId}
	</select>
</mapper>